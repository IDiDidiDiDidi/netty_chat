<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liudi.nettychat.mapper.UserMapper">

    <select id="selectByPrimaryKey" resultType="com.liudi.nettychat.entity.User">
    select
        *
    from t_users
    where id = #{id,jdbcType=VARCHAR}
    </select>
</mapper>